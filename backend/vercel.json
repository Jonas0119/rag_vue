{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "main.py": {
      "includeFiles": [
        "api/**",
        "core/**",
        "database/**",
        "services/**",
        "utils/**",
        "**/*.py",
        "pyproject.toml"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
